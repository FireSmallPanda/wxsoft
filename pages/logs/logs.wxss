page{
  background-color: #f3f3f3
}
.banner{
  display: block;
  width: 100%;
  height: 340rpx;
}
.block{
  background-color: #fff;
  margin-top: 15px;
}
.block-title{
  position: relative;
  font-size: 34rpx;
  line-height: 1.5;
  padding: 10rpx 30rpx;
}
.block-title::after{
  position: absolute;
  left: 20rpx;
  top:15rpx;
  content:"";
  width:10rpx;
  height:40rpx;
  background-color: #2e5e86;
}
.block-desc{
  color: #666;
  line-height: 1.6;
  font-size: 30rpx;
  padding: 0 30rox 30rpx;
}