<!--index.wxml-->
<swiper
  indicator-dots="{{indicatorDots}}"
  autoplay="{{autoplay}}"
  interval="{{interval}}"
  duration="{{duration}}">
  <block wx:for="{{imgUrls}}" wx:key="item">
    <swiper-item>
      <image src="{{item}}" class="slide-image" width="355" height="150" />
    </swiper-item>
  </block>
</swiper>
<view class='item-list'>
    <view class="pro-items" wx:for="{{proList}}"  wx:key="item.title"> 
      <image class="pro-logo" src='../../images/item.jpg'></image>
      <view class="pro-body">
        <view class="pro-title">{{item.title}}</view>
        <text class="pro-desc">{{item.desc}}</text>
        <view clsss="pro-footer">
          <image class='btn-detail' src='../../images/home.png'></image>
          <image class="btn-ask" src='../../images/log.png'></image>
        </view>
      </view>
    </view>
</view>
